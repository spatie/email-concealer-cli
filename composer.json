{
    "name": "spatie/email-concealer-cli",
    "description": "CLI tool for concealing e-mails in a file by replacing their domain",
    "keywords": [
        "spatie",
        "email-concealer-cli"
    ],
    "homepage": "https://github.com/spatie/email-concealer-cli",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0|^8.1",
        "spatie/email-concealer": "^1.0",
        "symfony/console": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\EmailConcealerCLI\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "email-concealer"
    ]
}
